{
  "name": "apimoviesjest",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "type": "module",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "start": "nodemon app.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "watch": "concurrently --kill-others npm:watch-*",
    "watch-node": "nodemon app.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}